import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int n,rev=0;
      System.out.println("Enter Number");
      n=sc.nextInt();
      int digit=n;
      while(n>0)
      {
        rev=rev*10+n%10;
        n=n/10;
      }
      if(digit==rev)
      {
          System.out.println("Yes");
      }
      else {
          System.out.println("No");
      }

    }

}
