import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int n,prod=1;
      System.out.println("Enter Number");
      n=sc.nextInt();

      while(n>0)

      {
        prod=prod*n;
        n=n-1;
      }

      System.out.println(prod);
    }

}
